<Window x:Class="Kalkulator_Wynarodzen_WPF.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Kalkulator_Wynarodzen_WPF"
        mc:Ignorable="d"
        Title="Kalkulator Wynagrodzeń" Height="700" Width="1000" Loaded="Window_Loaded" ResizeMode="CanMinimize" Icon="/calculator icon.png" ShowInTaskbar="False">
    <Window.Background>
        <ImageBrush ImageSource="/tlo.png"/>
    </Window.Background>
    <Grid Margin="0,0,0,-6">
     
        <Frame x:Name="frame" Content="Frame" HorizontalAlignment="Center" VerticalAlignment="Center" Margin="0,0,0,0" NavigationUIVisibility="Hidden"/>

    </Grid>
</Window>
